<!doctype html>
<html lang="pt-BR">
 <head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>lato - Sites Profissionais com SEO</title>
  <script src="/_sdk/element_sdk.js"></script>
  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    body {
      font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
      background-color: #000000;
      color: #ffffff;
      overflow-x: hidden;
      box-sizing: border-box;
    }

    html, body {
      height: 100%;
    }

    .hero-section {
      min-height: 100%;
      background-color: #000000;
      display: flex;
      flex-direction: column;
      position: relative;
    }

    .navbar {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 2rem 5%;
      position: relative;
      z-index: 10;
      border-bottom: 1px solid rgba(93, 224, 230, 0.1);
    }

    .logo {
      display: flex;
      align-items: center;
      gap: 1rem;
    }

    .logo-image {
      width: 50px;
      height: 50px;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      border-radius: 12px;
      display: flex;
      align-items: center;
      justify-content: center;
      box-shadow: 0 4px 15px rgba(0, 151, 178, 0.3);
    }

    .logo-text {
      font-size: 2.5rem;
      font-weight: 900;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
      letter-spacing: -0.02em;
    }

    .nav-links {
      display: flex;
      gap: 3rem;
      list-style: none;
    }

    .nav-links a {
      color: #e0e0e0;
      text-decoration: none;
      font-weight: 500;
      font-size: 1rem;
      transition: all 0.3s ease;
      position: relative;
    }

    .nav-links a:hover {
      color: #ffffff;
    }

    .nav-links a::after {
      content: '';
      position: absolute;
      bottom: -5px;
      left: 0;
      width: 0;
      height: 2px;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      transition: width 0.3s ease;
    }

    .nav-links a:hover::after {
      width: 100%;
    }

    .hero-content {
      flex: 1;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      text-align: center;
      padding: 4rem 5%;
      position: relative;
    }

    .hero-title {
      font-size: 5rem;
      font-weight: 900;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
      line-height: 1.1;
      margin-bottom: 2rem;
      max-width: 1000px;
      letter-spacing: -0.02em;
    }

    .hero-subtitle {
      font-size: 1.5rem;
      color: #b0b0b0;
      line-height: 1.6;
      margin-bottom: 4rem;
      max-width: 800px;
      font-weight: 400;
    }

    .cta-button {
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      color: #ffffff;
      padding: 1.5rem 4rem;
      font-size: 1.2rem;
      font-weight: 700;
      border: none;
      border-radius: 60px;
      cursor: pointer;
      transition: all 0.4s ease;
      box-shadow: 0 15px 40px rgba(0, 151, 178, 0.3);
      text-transform: uppercase;
      letter-spacing: 1px;
      position: relative;
      overflow: hidden;
    }

    .cta-button::before {
      content: '';
      position: absolute;
      top: 0;
      left: -100%;
      width: 100%;
      height: 100%;
      background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
      transition: left 0.6s ease;
    }

    .cta-button:hover::before {
      left: 100%;
    }

    .cta-button:hover {
      transform: translateY(-5px);
      box-shadow: 0 20px 50px rgba(0, 151, 178, 0.4);
    }

    .tech-grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 3rem;
      margin-top: 6rem;
      max-width: 1200px;
      width: 100%;
    }

    .tech-card {
      background: rgba(18, 18, 18, 0.8);
      border: 1px solid rgba(93, 224, 230, 0.2);
      border-radius: 20px;
      padding: 3rem 2rem;
      text-align: center;
      transition: all 0.4s ease;
      position: relative;
      overflow: hidden;
    }

    .tech-card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      height: 3px;
      background: linear-gradient(135deg, #5de0e6 0%, #004aad 100%);
      transform: scaleX(0);
      transition: transform 0.4s ease;
    }

    .tech-card:hover::before {
      transform: scaleX(1);
    }

    .tech-card:hover {
      background: rgba(18, 18, 18, 1);
      border-color: rgba(93, 224, 230, 0.4);
      transform: translateY(-10px);
    }

    .tech-icon {
      margin-bottom: 1.5rem;
      display: flex;
      justify-content: center;
      align-items: center;
    }

    .tech-title {
      font-size: 1.4rem;
      font-weight: 700;
      color: #ffffff;
      margin-bottom: 1rem;
    }

    .tech-description {
      font-size: 1rem;
      color: #a0a0a0;
      line-height: 1.6;
    }

    .stats-section {
      display: flex;
      justify-content: space-around;
      margin-top: 5rem;
      max-width: 800px;
      width: 100%;
    }

    .stat-item {
      text-align: center;
    }

    .stat-number {
      font-size: 3rem;
      font-weight: 900;
      background: linear-gradient(135deg, #5de0e6 0%, #004aad 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
      display: block;
    }

    .stat-label {
      font-size: 0.9rem;
      color: #808080;
      text-transform: uppercase;
      letter-spacing: 1px;
      margin-top: 0.5rem;
    }

    .floating-particles {
      position: absolute;
      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
      pointer-events: none;
      overflow: hidden;
    }

    .particle {
      position: absolute;
      width: 4px;
      height: 4px;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      border-radius: 50%;
      animation: float-particle 15s infinite linear;
      opacity: 0.6;
    }

    .particle:nth-child(1) { left: 10%; animation-delay: 0s; }
    .particle:nth-child(2) { left: 20%; animation-delay: 2s; }
    .particle:nth-child(3) { left: 30%; animation-delay: 4s; }
    .particle:nth-child(4) { left: 40%; animation-delay: 6s; }
    .particle:nth-child(5) { left: 50%; animation-delay: 8s; }
    .particle:nth-child(6) { left: 60%; animation-delay: 10s; }
    .particle:nth-child(7) { left: 70%; animation-delay: 12s; }
    .particle:nth-child(8) { left: 80%; animation-delay: 14s; }

    @keyframes float-particle {
      0% {
        transform: translateY(100vh) scale(0);
        opacity: 0;
      }
      10% {
        opacity: 0.6;
        transform: scale(1);
      }
      90% {
        opacity: 0.6;
      }
      100% {
        transform: translateY(-100px) scale(0);
        opacity: 0;
      }
    }

    .contact-section {
      margin-top: 4rem;
      max-width: 600px;
      width: 100%;
      text-align: center;
    }

    .contact-info {
      display: flex;
      justify-content: center;
      gap: 4rem;
      flex-wrap: wrap;
    }

    .contact-item {
      display: flex;
      align-items: center;
      gap: 1rem;
      color: #ffffff;
      font-size: 1.1rem;
      font-weight: 500;
      transition: all 0.3s ease;
    }

    .contact-item:hover {
      transform: translateY(-2px);
    }

    .contact-item span {
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
      font-weight: 600;
    }

    .portfolio-section {
      margin-top: 6rem;
      max-width: 1400px;
      width: 100%;
    }

    .portfolio-title {
      font-size: 2.5rem;
      font-weight: 800;
      text-align: center;
      margin-bottom: 1rem;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .portfolio-subtitle {
      font-size: 1.2rem;
      color: #b0b0b0;
      text-align: center;
      margin-bottom: 4rem;
      max-width: 600px;
      margin-left: auto;
      margin-right: auto;
    }

    .portfolio-grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 2.5rem;
      margin-bottom: 4rem;
    }

    .portfolio-card {
      background: rgba(18, 18, 18, 0.9);
      border: 1px solid rgba(93, 224, 230, 0.2);
      border-radius: 20px;
      overflow: hidden;
      transition: all 0.4s ease;
      position: relative;
    }

    .portfolio-card:hover {
      transform: translateY(-10px);
      border-color: rgba(93, 224, 230, 0.4);
      box-shadow: 0 20px 40px rgba(0, 151, 178, 0.2);
    }

    .portfolio-image {
      height: 250px;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      position: relative;
      overflow: hidden;
    }

    .portfolio-overlay {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: rgba(0, 0, 0, 0.7);
      display: flex;
      align-items: center;
      justify-content: center;
      opacity: 0;
      transition: all 0.4s ease;
    }

    .portfolio-card:hover .portfolio-overlay {
      opacity: 1;
    }

    .portfolio-info {
      text-align: center;
      color: white;
    }

    .portfolio-info h3 {
      font-size: 1.5rem;
      font-weight: 700;
      margin-bottom: 0.5rem;
    }

    .portfolio-info p {
      font-size: 1rem;
      margin-bottom: 1rem;
      opacity: 0.9;
    }

    .portfolio-stats {
      display: flex;
      gap: 1rem;
      justify-content: center;
    }

    .portfolio-stats span {
      background: rgba(255, 255, 255, 0.2);
      padding: 0.5rem 1rem;
      border-radius: 20px;
      font-size: 0.9rem;
      font-weight: 600;
    }

    .portfolio-details {
      padding: 2rem;
    }

    .portfolio-details h4 {
      font-size: 1.3rem;
      font-weight: 700;
      color: #ffffff;
      margin-bottom: 1rem;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .portfolio-details p {
      font-size: 1rem;
      color: #b0b0b0;
      line-height: 1.6;
      margin-bottom: 1.5rem;
    }

    .portfolio-tags {
      display: flex;
      gap: 0.8rem;
      flex-wrap: wrap;
    }

    .portfolio-tags span {
      background: rgba(93, 224, 230, 0.1);
      color: #5de0e6;
      padding: 0.4rem 1rem;
      border-radius: 15px;
      font-size: 0.8rem;
      font-weight: 500;
      border: 1px solid rgba(93, 224, 230, 0.3);
    }

    .portfolio-cta {
      text-align: center;
    }

    .portfolio-button {
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      color: #ffffff;
      padding: 1.2rem 3rem;
      font-size: 1.1rem;
      font-weight: 700;
      border: none;
      border-radius: 50px;
      cursor: pointer;
      transition: all 0.4s ease;
      box-shadow: 0 10px 30px rgba(0, 151, 178, 0.3);
      text-transform: uppercase;
      letter-spacing: 1px;
    }

    .portfolio-button:hover {
      transform: translateY(-3px);
      box-shadow: 0 15px 40px rgba(0, 151, 178, 0.4);
    }

    .testimonials-section {
      margin-top: 6rem;
      max-width: 1200px;
      width: 100%;
    }

    .testimonials-title {
      font-size: 2.5rem;
      font-weight: 800;
      text-align: center;
      margin-bottom: 4rem;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .testimonials-grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 2.5rem;
    }

    .testimonial-card {
      background: rgba(18, 18, 18, 0.9);
      border: 1px solid rgba(93, 224, 230, 0.2);
      border-radius: 20px;
      padding: 2.5rem 2rem;
      text-align: left;
      transition: all 0.4s ease;
      position: relative;
      overflow: hidden;
    }

    .testimonial-card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      height: 3px;
      background: linear-gradient(135deg, #5de0e6 0%, #004aad 100%);
      transform: scaleX(0);
      transition: transform 0.4s ease;
    }

    .testimonial-card:hover::before {
      transform: scaleX(1);
    }

    .testimonial-card:hover {
      background: rgba(18, 18, 18, 1);
      border-color: rgba(93, 224, 230, 0.4);
      transform: translateY(-5px);
    }

    .stars {
      display: flex;
      gap: 0.3rem;
      margin-bottom: 1.5rem;
    }

    .testimonial-text {
      font-size: 1rem;
      color: #e0e0e0;
      line-height: 1.6;
      margin-bottom: 2rem;
      font-style: italic;
    }

    .testimonial-author {
      font-size: 1.1rem;
      font-weight: 700;
      color: #ffffff;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .social-float-container {
      position: fixed;
      bottom: 30px;
      right: 30px;
      display: flex;
      flex-direction: column;
      gap: 15px;
      z-index: 1000;
    }

    .whatsapp-float {
      width: 60px;
      height: 60px;
      background: linear-gradient(135deg, #25d366 0%, #128c7e 100%);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      box-shadow: 0 8px 25px rgba(37, 211, 102, 0.4);
      cursor: pointer;
      transition: all 0.3s ease;
      animation: pulse-whatsapp 2s infinite;
    }

    .whatsapp-float:hover {
      transform: scale(1.1);
      box-shadow: 0 12px 35px rgba(37, 211, 102, 0.6);
    }

    .whatsapp-float svg {
      width: 32px;
      height: 32px;
      fill: white;
    }

    .instagram-float {
      width: 60px;
      height: 60px;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      box-shadow: 0 8px 25px rgba(0, 151, 178, 0.4);
      cursor: pointer;
      transition: all 0.3s ease;
      animation: pulse-instagram 2s infinite;
      animation-delay: 1s;
    }

    .instagram-float:hover {
      transform: scale(1.1);
      box-shadow: 0 12px 35px rgba(0, 151, 178, 0.6);
    }

    .instagram-float svg {
      width: 32px;
      height: 32px;
      fill: white;
    }

    @keyframes pulse-whatsapp {
      0% {
        box-shadow: 0 8px 25px rgba(37, 211, 102, 0.4);
      }
      50% {
        box-shadow: 0 8px 25px rgba(37, 211, 102, 0.4), 0 0 0 10px rgba(37, 211, 102, 0.1);
      }
      100% {
        box-shadow: 0 8px 25px rgba(37, 211, 102, 0.4);
      }
    }

    @keyframes pulse-instagram {
      0% {
        box-shadow: 0 8px 25px rgba(0, 151, 178, 0.4);
      }
      50% {
        box-shadow: 0 8px 25px rgba(0, 151, 178, 0.4), 0 0 0 10px rgba(0, 151, 178, 0.1);
      }
      100% {
        box-shadow: 0 8px 25px rgba(0, 151, 178, 0.4);
      }
    }

    /* Estilos do Rodapé */
    .footer {
      background: linear-gradient(135deg, #0a0a0a 0%, #1a1a1a 100%);
      border-top: 1px solid rgba(93, 224, 230, 0.1);
      margin-top: 6rem;
    }

    .footer-content {
      max-width: 1400px;
      margin: 0 auto;
      padding: 4rem 5% 2rem;
    }

    .footer-grid {
      display: grid;
      grid-template-columns: 2fr 1fr 1fr 1.5fr;
      gap: 4rem;
      margin-bottom: 3rem;
    }

    .footer-column {
      display: flex;
      flex-direction: column;
    }

    .footer-logo {
      display: flex;
      align-items: center;
      gap: 1rem;
      margin-bottom: 1.5rem;
    }

    .footer-logo-image {
      width: 40px;
      height: 40px;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      border-radius: 10px;
      display: flex;
      align-items: center;
      justify-content: center;
    }

    .footer-logo-text {
      font-size: 2rem;
      font-weight: 900;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .footer-description {
      color: #b0b0b0;
      line-height: 1.6;
      margin-bottom: 2rem;
      font-size: 1rem;
    }

    .footer-social {
      display: flex;
      gap: 1rem;
    }

    .social-link {
      width: 45px;
      height: 45px;
      background: rgba(93, 224, 230, 0.1);
      border: 1px solid rgba(93, 224, 230, 0.2);
      border-radius: 12px;
      display: flex;
      align-items: center;
      justify-content: center;
      color: #5de0e6;
      transition: all 0.3s ease;
      text-decoration: none;
    }

    .social-link:hover {
      background: rgba(93, 224, 230, 0.2);
      border-color: rgba(93, 224, 230, 0.4);
      transform: translateY(-2px);
    }

    .footer-title {
      font-size: 1.3rem;
      font-weight: 700;
      color: #ffffff;
      margin-bottom: 1.5rem;
      background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .footer-links {
      list-style: none;
      display: flex;
      flex-direction: column;
      gap: 0.8rem;
    }

    .footer-link {
      color: #b0b0b0;
      text-decoration: none;
      font-size: 1rem;
      transition: all 0.3s ease;
      position: relative;
    }

    .footer-link:hover {
      color: #ffffff;
      padding-left: 0.5rem;
    }

    .footer-contact {
      display: flex;
      flex-direction: column;
      gap: 1rem;
    }

    .contact-item-footer {
      display: flex;
      align-items: center;
      gap: 0.8rem;
      color: #b0b0b0;
      font-size: 1rem;
    }

    .contact-item-footer svg {
      color: #5de0e6;
      flex-shrink: 0;
    }

    .footer-divider {
      height: 1px;
      background: linear-gradient(90deg, transparent, rgba(93, 224, 230, 0.3), transparent);
      margin: 2rem 0;
    }

    .footer-bottom {
      padding-top: 2rem;
    }

    .footer-bottom-content {
      display: flex;
      justify-content: space-between;
      align-items: center;
      flex-wrap: wrap;
      gap: 1rem;
    }

    .footer-copyright {
      color: #808080;
      font-size: 0.9rem;
    }

    .footer-legal {
      display: flex;
      gap: 2rem;
    }

    .footer-legal-link {
      color: #808080;
      text-decoration: none;
      font-size: 0.9rem;
      transition: color 0.3s ease;
    }

    .footer-legal-link:hover {
      color: #ffffff;
    }

    @media (max-width: 768px) {
      .hero-title {
        font-size: 3rem;
      }

      .hero-subtitle {
        font-size: 1.2rem;
      }

      .tech-grid {
        grid-template-columns: 1fr;
        gap: 2rem;
        margin-top: 4rem;
      }

      .contact-info {
        flex-direction: column;
        gap: 2rem;
      }

      .portfolio-grid {
        grid-template-columns: 1fr;
        gap: 2rem;
      }

      .portfolio-title {
        font-size: 2rem;
      }

      .portfolio-subtitle {
        font-size: 1rem;
      }

      .testimonials-grid {
        grid-template-columns: 1fr;
        gap: 2rem;
      }

      .testimonials-title {
        font-size: 2rem;
      }

      .stats-section {
        flex-direction: column;
        gap: 2rem;
      }

      .nav-links {
        display: none;
      }

      .navbar {
        justify-content: center;
      }

      .logo-image {
        width: 40px;
        height: 40px;
      }

      .logo-text {
        font-size: 2rem;
      }

      .cta-button {
        padding: 1.2rem 3rem;
        font-size: 1rem;
      }

      .social-float-container {
        bottom: 20px;
        right: 20px;
        gap: 12px;
      }

      .whatsapp-float, .instagram-float {
        width: 55px;
        height: 55px;
      }

      .whatsapp-float svg, .instagram-float svg {
        width: 28px;
        height: 28px;
      }

      /* Responsivo do Rodapé */
      .footer-content {
        padding: 3rem 5% 1.5rem;
      }

      .footer-grid {
        grid-template-columns: 1fr;
        gap: 2.5rem;
        margin-bottom: 2rem;
      }

      .footer-bottom-content {
        flex-direction: column;
        text-align: center;
        gap: 1.5rem;
      }

      .footer-legal {
        justify-content: center;
      }

      .footer-social {
        justify-content: flex-start;
      }
    }
  </style>
