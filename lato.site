      `;

      const content = document.createElement('div');
      content.style.cssText = `
        background: #121212;
        border: 2px solid;
        border-image: linear-gradient(135deg, #0097b2 0%, #7ed957 100%) 1;
        padding: 3rem;
        border-radius: 20px;
        text-align: center;
        max-width: 500px;
        width: 90%;
      `;

      content.innerHTML = `
        <h3 style="color: #ffffff; margin-bottom: 1.5rem; font-size: 2rem; background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text;">Vamos Criar Seu Site!</h3>
        <p style="color: #b0b0b0; margin-bottom: 2rem; line-height: 1.6;">Gostou dos nossos projetos? Entre em contato conosco e vamos criar um site incrível para o seu negócio também!</p>
        <button id="close-portfolio-modal" style="background: linear-gradient(135deg, #0097b2 0%, #7ed957 100%); color: white; border: none; padding: 1rem 2.5rem; border-radius: 30px; cursor: pointer; font-weight: 700; font-size: 1rem;">Quero Meu Site</button>
      `;

      modal.appendChild(content);
      document.body.appendChild(modal);

      document.getElementById('close-portfolio-modal').addEventListener('click', function() {
        document.body.removeChild(modal);
        // Abrir WhatsApp com mensagem específica sobre criação de site
        const phoneNumber = '554198869-2013';
        const message = 'Olá! Vi o portfólio da lato e gostaria de criar um site profissional para meu negócio também!';
        const whatsappUrl = `https://wa.me/${phoneNumber}?text=${encodeURIComponent(message)}`;
        window.open(whatsappUrl, '_blank', 'noopener,noreferrer');
      });

      modal.addEventListener('click', function(e) {
        if (e.target === modal) {
          document.body.removeChild(modal);
        }
      });
    });
